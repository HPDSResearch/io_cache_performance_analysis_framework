[randomread]

readwrite=randread
blocksize=4k
direct=1
ioengine=libaio

iodepth=16
numjobs=64

#filename=/dev/sdb

norandommap
group_reporting=1
lat_percentiles=1

offset=0
size=55g
io_size=9g
random_distribution=random

##for logging##
log_avg_msec=10000
